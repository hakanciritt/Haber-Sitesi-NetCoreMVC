<!doctype html>
<html>
<head>
    <!-- Basic -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Site Metas -->
    <title>@ViewData["Title"]</title>

    <!-- Site Icons -->
    <link rel="shortcut icon" href="~/haber/images/favicon.ico" type="image/x-icon" />
    <link rel="apple-touch-icon" href="~/haber/images/apple-touch-icon.png">

    <!-- Design fonts -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet">

    <!-- Bootstrap core CSS -->
    <link href="~/haber/css/bootstrap.css" rel="stylesheet">

    <!-- FontAwesome Icons core CSS -->
    <link href="~/haber/css/font-awesome.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="~/haber/style.css" rel="stylesheet">

    <!-- Responsive styles for this template -->
    <link href="~/haber/css/responsive.css" rel="stylesheet">

    <!-- Colors for this template -->
    <link href="~/haber/css/colors.css" rel="stylesheet">

    <!-- Version Tech CSS for this template -->
    <link href="~/haber/css/version/tech.css" rel="stylesheet">
</head>
<body>

    <div id="wrapper">
        <header class="tech-header header">
            <div class="container-fluid">
                @await Component.InvokeAsync("Nav")
            </div>
        </header>

        <div>
            @RenderBody()
        </div>

        @await Component.InvokeAsync("Footer")

    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/haber/js/tether.min.js"></script>
    <script src="~/haber/js/bootstrap.min.js"></script>
    <script src="~/haber/js/custom.js"></script>
    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>


